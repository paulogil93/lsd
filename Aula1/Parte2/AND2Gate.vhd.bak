--BIBLIOTECAS
library IEEE;
use IEEE.STD_LOGIC_1164.all;

--INTERFACE(PORTAS)
entity AND2Gate is
	port(port_in_0 : in std_logic;
		  port_in_1 : in std_logic;
		  port_out  : in std_logic);
end AND2Gate;

--IMPLEMENTAÇÃO
architecture Behavioral of AND2Gate is
begin
		port_out <= port_in_0 and port_in_1;
end Behavioral;